{
    "name": "dhruv125/ecommerce.site",
    "description": "This is my Project to Create a working E-Commerce site with login system, admin-page, and working carts page",
    "type": "project",
    "license": "MIT",
    "require": {
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Dhruv125\\EcommerceSite\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sanatani-Dhruv",
            "email": "dhruvdds125@gmail.com"
        }
    ],
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
    ,
    "config": {
        "optimize-autoloader": false
    },
    "minimum-stability": "stable"
}
